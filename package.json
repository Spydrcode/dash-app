{
  "name": "dash-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:network": "next dev -H 0.0.0.0",
    "dev:full": "concurrently \"npm run dev:network\" \"npm run mcp:start\"",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "mcp:install": "cd mcp-servers && npm run install:all",
    "mcp:start": "cd mcp-servers && npm run start:all",
    "ollama:setup": "ollama pull llama2:13b-instruct && ollama pull mistral:7b-instruct",
    "deploy:office": "node deploy-helper.js && deploy-with-ngrok.bat",
    "start:tunnel": "concurrently \"npm start\" \"ngrok http 3000\""
  },
  "dependencies": {
    "@headlessui/react": "^2.2.8",
    "@heroicons/react": "^2.1.1",
    "@supabase/supabase-js": "^2.58.0",
    "axios": "^1.6.7",
    "chart.js": "^4.4.1",
    "date-fns": "^3.3.1",
    "dotenv": "^17.2.2",
    "form-data": "^4.0.4",
    "next": "15.5.4",
    "node-fetch": "^2.7.0",
    "openai": "^6.0.0",
    "react": "19.1.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.4.1",
    "tesseract.js": "^6.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^8.2.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5"
  }
}
